<h1>Cats</h1>

<table>
	<tr>
	<td>name</td>
	</tr>

	<% for cat in @cats %>
		<tr>
		<td><%= cat.name %></td>
		</tr>
	<% end %>
</table>

<h1>Todos</h1>

<table>
	<tr>
	<td>tasks</td>
	<td>finished</td>
	</tr>

	<% for todo in @todos %>
		<tr>
		<td><%= todo.tasks %></td>
		<td><%= todo.finished %></td>
		</tr>
	<% end %>
</table>

<h1>Users</h1>

<table>
	<tr>
	<td>username</td>
	<td>email</td>
	<td>age</td>
	</tr>

	<% for user in @users %>
		<tr>
		<td><%= user.username %></td>
		<td><%= user.email %></td>
		<td><%= user.age %></td>
		</tr>
	<% end %>
</table>
